.accordions-vertical .arrows{display: block;height: 14px;position: relative;float:right;right: -15px;top:3px;width: 14px; background-image: url('../../images/nav-arrows.png');background-position: -8px -39px;background-repeat: no-repeat;}
.accordions-vertical div.accordion-title {background-color: #FDFDFD;cursor:pointer; font-size: 13px;font-weight: 300;line-height: 21px;margin: 0;padding: 14px 30px 14px 18px;}
.ui-accordion-header {border:1px solid #d8d8d8;border-bottom:none;}
