.cv-tabs-nav span:last-child a {border-bottom:1px solid #e0e0e0;}
.cv-tabs-nav span a:hover {background: #FFFFFF;}
.tabs-horizontal {clear: both;margin-top: 15px;}
.tabs-horizontal ul.tabs {list-style: none !important;margin: 0 !important;padding: 0;height: 39px;}
.tabs-horizontal ul.tabs li {float: left;line-height: 38px;overflow: hidden;padding: 0;position: relative;}
.tabs-horizontal ul.tabs a:first-child{background: #FBFBFB; border-left: 1px solid #E0E0E0; padding: 0 20px;color: #373737;display: block;overflow: hidden;}
.tabs-horizontal ul.tabs li:last-child a{border-right: 1px solid #E0E0E0;}
.tabs-horizontal ul.tabs a{border:1px solid #e0e0e0;border-right:none;border-left:none; padding: 0 20px;text-decoration: none;background-color: #F8F8F8;}
.tabs-horizontal ul.tabs a.active { color: #169FE6;border-top: 1px solid #169FE6;background: #FFFFFF;height: 40px;border-bottom:none;}
.tabs-horizontal ul.tabs a:hover {background: #FFFFFF;margin: 0; }
.tabs-horizontal .panes { border: 1px solid #D8D8D8;box-shadow: 0 2px 0 0 rgba(0, 0, 0, 0.03);}
.tabs-horizontal .pane { height: auto;margin-top: 0;opacity: 1;padding: 20px;visibility: visible; }

